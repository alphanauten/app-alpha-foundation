{% block alpha_marketing_banner_list %}
    <sw-page class="alphanauten-marketing-banner-list">
        {% block alpha_marketing_banner_list_smart_bar_actions %}
            <template #smart-bar-actions>
                <sw-button variant="primary" :router-link="{ name: 'alphanauten.marketing.banner.create' }">
                    {{ $tc('marketing-banner.list.addButtonText') }}
                </sw-button>
            </template>
        {% endblock %}

        <template #content>
            {% block alpha_marketing_banner_list_content %}
                <sw-entity-listing
                        v-if="entities"
                        :items="entities"
                        :repository="entityRepository"
                        :show-selection="false"
                        :allow-inline-edit="false"
                        :columns="columns"
                >
                </sw-entity-listing>
            {% endblock %}
        </template>
    </sw-page>
{% endblock %}
